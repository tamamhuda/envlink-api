    "seed:plans": "node dist/scripts/seed.js",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev:docker": "docker-compose --env-file=.env.development -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "down:dev:docker": "docker-compose --env-file=.env.development -f docker-compose.yml -f docker-compose.dev.yml down -v",
    "start:prod:docker": "docker-compose --env-file .env.production -f docker-compose.yml -f docker-compose.prod.yml up --build",
    "down:prod:docker": "docker-compose --env-file .env.production -f docker-compose.yml -f docker-compose.prod.yml down -v",
    "start:dev:local": "NODE_ENV=local nest start --watch",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
